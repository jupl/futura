{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "bin": "dist/bin/app.js",
  "scripts": {
    "package": "npm run build:prod && npm pack && npm run clean",
    "clean": "trash dist/bin dist/next",
    "analyze": "cross-env ANALYZE=true npm run build:next",
    "analyze:server": "cross-env ANALYZE=server npm run build:next",
    "analyze:client": "cross-env ANALYZE=client npm run build:next",
    "coverage": "npm run test -- --coverage",
    "coverage:watch": "npm run coverage -- --watch",
    "lint": "tsc && npm run lint:file",
    "lint:file": "tslint --project tsconfig.json",
    "test": "jest --config .jestrc.json",
    "test:update": "npm run test -- -u",
    "test:watch": "npm run test -- --watch",
    "server": "npm run server:dev:combo",
    "server:dev:combo": "cross-env NODE_ENV=development ESM_DISABLE_CACHE=true IS_SERVER=true node --inspect -r esm -r isomorphic-fetch -r reflect-metadata -r dotenv/config -r tsconfig-paths/register -r ts-node/register/type-check src/bin/app.ts",
    "server:dev:api": "cross-env API_ONLY=true ESM_DISABLE_CACHE=true nodemon src/bin/app.ts",
    "server:dev:ui": "cross-env next -p 3001",
    "server:prod": "next build && cross-env NODE_ENV=production ESM_DISABLE_CACH=true node -r esm -r isomorphic-fetch -r reflect-metadata -r dotenv/config -r tsconfig-paths/register -r ts-node/register/transpile-only src/bin/app",
    "server:story": "start-storybook -p 9001",
    "build:base": "npm run clean && next build && cross-env TS_NODE_PROJECT=tsconfig.base.json webpack",
    "build:dev": "cross-env NODE_ENV=development npm run build:base",
    "build:prod": "cross-env NODE_ENV=production npm run build:base"
  },
  "bundledDependencies": [
    "apollo-boost",
    "apollo-link-schema",
    "apollo-server-hapi",
    "dotenv",
    "good",
    "good-console",
    "good-squeeze",
    "graphql-tag",
    "hapi",
    "isomorphic-fetch",
    "lodash",
    "next",
    "react",
    "react-apollo",
    "react-dom",
    "reflect-metadata",
    "rotating-file-stream",
    "styled-components",
    "type-graphql"
  ],
  "dependencies": {
    "apollo-boost": "^0.1.23",
    "apollo-link-schema": "^1.1.4",
    "apollo-server-hapi": "^2.3.1",
    "dotenv": "^6.2.0",
    "good": "^8.1.1",
    "good-console": "^7.1.0",
    "good-squeeze": "^5.1.0",
    "graphql-tag": "^2.10.0",
    "hapi": "^17.8.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.11",
    "next": "^7.0.2",
    "react": "^16.7.0",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.7.0",
    "reflect-metadata": "^0.1.12",
    "rotating-file-stream": "^1.3.9",
    "styled-components": "^4.1.3",
    "type-graphql": "^0.16.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@jupl/tslint-config": "^0.6.5",
    "@storybook/react": "^4.1.4",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/globule": "^1.1.3",
    "@types/h2o2": "^8.1.0",
    "@types/hapi": "^17.8.2",
    "@types/jest": "^23.3.10",
    "@types/lodash": "^4.14.119",
    "@types/next": "^7.0.6",
    "@types/react": "^16.7.18",
    "@types/storybook__react": "^4.0.0",
    "@types/styled-components": "^4.1.4",
    "@types/webpack-env": "^1.13.6",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-css": "^1.0.0",
    "@zeit/next-typescript": "^1.1.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-graphql-tag": "^1.6.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-styled-components": "^1.10.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "esm": "^3.0.84",
    "globule": "^1.2.1",
    "h2o2": "^8.1.2",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-enzyme": "^7.0.1",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.18.9",
    "normalize.css": "^8.0.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2",
    "typescript-tslint-plugin": "^0.2.1",
    "wcb": "^0.15.2",
    "webpack-cli": "^3.1.2"
  }
}
