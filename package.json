    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-styled-components": "^1.7.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "graphql-tag": "^2.9.2",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-enzyme": "^6.0.4",
    "lint-staged": "^7.2.2",
    "lodash": "^4.17.11",
    "micro-dev": "^3.0.0",
    "normalize.css": "^8.0.0",
